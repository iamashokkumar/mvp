{
	"welcomeFile": "/mvp-ui/index.html",
	"authenticationMethod": "route",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
			"source": "^/mvp-ui/(.*)$",
			"target": "$1",
			"localDir": "webapp"
		}, {
			"source": "(.*)(.xsjs)",
			"destination": "mvp-api",
			"csrfProtection": true,
			"authenticationType": "xsuaa"
		},
		{
			"source": "(.*)(.xsodata)",
			"destination": "mvp-api",
			"authenticationType": "xsuaa"
		}
		]
}