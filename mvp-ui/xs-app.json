{
	"welcomeFile": "/mvp-ui/index.html",
	"authenticationMethod": "route",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
			"source": "^/mvp-ui/(.*)$",
			"target": "$1",
			"localDir": "webapp"
		}, {
			"source": "/mvp-api/(.*)(.xsjs)",
			"destination": "mvp-api",
			"authenticationType": "xsuaa"
		}, {
			"source": "/mvp-api/demo.xsodata/.*$",
			"destination": "mvp-api",
			"authenticationType": "xsuaa"
		}
		]
}